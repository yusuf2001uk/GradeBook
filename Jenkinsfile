pipeline {
agent any 
stages {
stage ("checkout") {
steps {
echo 'Checking out the Application...'
}
}
stage ("build") {
steps {
echo 'Building the Application...'
}
}
stage ("test") {
steps {
echo 'Testing the Application...'
}
}
stage ("deploy") {
steps {
echo 'Deploying the Application..'
}
}
}
}
